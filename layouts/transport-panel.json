{
  "createdWith": "Open Stage Control",
  "version": "1.29.5",
  "type": "session",
  "content": {
    "type": "root",
    "tabs": [
      {
        "type": "tab",
        "label": "main",
        "widgets": [
          {
            "type": "button",
            "id": "play_button",
            "label": "Play",
            "address": "/midi/note",
            "preArgs": [1, 60, 127],
            "target": "midi:OSC-server"
          },
          {
            "type": "button", 
            "id": "record_button",
            "label": "Record",
            "address": "/midi/note",
            "preArgs": [1, 61, 127],
            "target": "midi:OSC-server"
          },
          {
            "type": "fader",
            "id": "master_volume",
            "label": "Master Vol",
            "range": {"min": 0, "max": 127},
            "address": "/midi/cc",
            "preArgs": [1, 7],
            "target": "midi:OSC-server"
          }
        ],
        "decimals": 2,
        "colorWidget": "auto",
        "lock": false,
        "id": "tab_1",
        "visible": true,
        "interaction": true,
        "comments": "",
        "colorText": "auto",
        "colorFill": "auto",
        "borderRadius": "auto",
        "padding": "auto",
        "html": "",
        "css": "",
        "colorBg": "auto",
        "layout": "default",
        "justify": "start",
        "gridTemplate": "",
        "contain": true,
        "scroll": true,
        "innerPadding": true,
        "tabsPosition": "top",
        "variables": "@{parent.variables}",
        "traversing": false,
        "value": "",
